{
    "name": "bluegg/websitedocumentation",
    "description": "Craft Plugin: Creates two links within admin for Styleguide and CMS Guide",
    "type": "craft-plugin",
    "version": "1.2.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Bluegg",
            "homepage": "https://bluegg.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
		"craftcms/redactor": "^2.9.0"
    },
    "autoload": {
        "psr-4": {
