{
	"name": "towardstudio/websitedocumentation",
	"description": "Add Style Guide & CMS Guide to your website",
	"type": "craft-plugin",
	"version": "3.1.3",
	"license": "MIT",
	"authors": [
		{
			"name": "Toward Studio",
			"homepage": "https://toward.studio"
		}
	],
	"require": {
		"craftcms/cms": "^4.0.0",
		"craftcms/redactor": "^3"
	},
	"autoload": {
		"psr-4": {
			"towardstudio\\websitedocumentation\\": "src/"
		}
	},
	"extra": {
		"name": "Website Documentation",
		"handle": "websitedocumentation",
		"developer": "Toward Studio",
		"developerUrl": "https://toward.studio",
		"documentationUrl": "https://github.com/towardstudio/websitedocumentation/blob/main/README.md",
		"changelogUrl": "https://raw.githubusercontent.com/towardstudio/websitedocumentation/main/CHANGELOG.md",
		"class": "towardstudio\\websitedocumentation\\WebsiteDocumentation"
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}
