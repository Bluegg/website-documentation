        "developer": "Bluegg",
        "developerUrl": "https://bluegg.co.uk",
        "documentationUrl": "https://github.com/websitedocumentation/websitedocumentation/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/websitedocumentation/websitedocumentation/master/CHANGELOG.md",
        "class": "bluegg\\websitedocumentation\\WebsiteDocumentation"
    }
